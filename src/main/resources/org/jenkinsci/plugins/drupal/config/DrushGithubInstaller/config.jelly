<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:entry title="${%Version}" field="version">
    <j:choose>
      <j:set var="releases" value="${descriptor.installableReleases}"/>
      <select>
        <j:forEach var="release" items="${releases}">
          <f:option value="${release.id}" selected="${release.equals(instance.id)}">${release.name}</f:option>
        </j:forEach>
        </select>
    </j:choose>
  </f:entry>
</j:jelly>